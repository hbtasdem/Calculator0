{
  "expo": {
    "name": "Calculator0",
    "slug": "Calculator0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "cipher",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cipher.Calculator0",
      "infoPlist": {
        "NSCameraUsageDescription": "Scan documents for secure storage",
        "NSPhotoLibraryUsageDescription": "Save evidence securely",
        "NSPhotoLibraryAddUsageDescription": "Save encrypted documents",
        "NSFaceIDUsageDescription": "Use Face ID for secure access"
      }
    },
    "android": {
      "package": "com.cipher.Calculator0",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "VIBRATE",
        "INTERNET"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Access photos to store evidence securely",
          "cameraPermission": "Access camera to scan documents"
        }
      ],
      "expo-document-picker",
      "expo-file-system",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Use Face ID for secure access"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}